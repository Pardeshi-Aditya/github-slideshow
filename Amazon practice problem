//Milly and Chocolates again 

#include <iostream>
using namespace std;

int main(){
    int p;
    cin>>p;
    int m[p];
    for(int i=0;i<p;i++){
        cin>>m[i];
    }
    int n,ans;
    cin>>n;
    while(n!=0){
        cin>>ans;
        int b=0;
        while(ans>0){
            ans -=m[b];
            b++;
        }
        cout<<b<<endl;
        n--;
    }
    return 0;
}
